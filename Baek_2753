#include <iostream>
#include <cmath>
#include <stack>
#include <string>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int year = 0;
    int result;

    cin >> year;

    if(year % 400 == 0)
        result = 1;
    else if(year % 100 == 0)
        result = 0;
    else if(year % 4 == 0)
        result = 1;
    else
        result = 0;

    cout << result;

    return 0;
}
