#include <iostream>
#include <cmath>
#include <stack>
#include <string>


using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int num1, num2 = 0;
    int a, b, c, d = 0;

    cin >> num1 >> num2;

    d = num1 * num2;

    c = num1 * (num2 / 100);
    num2 = num2 % 100;

    b = num1 * (num2 / 10);
    num2 = num2 % 10;

    a = num1 * num2;

    cout << a << '\n' << b << '\n' << c << '\n' << d;

    return 0;
}


